{
    "containerDefinitions": [
        {
            "name": "angular-task",
            "image": "<IMAGE1_NAME>",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/AngularTD",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost/ || exit 1"
                ],
                "interval": 30,
                "retries": 3
            }
        }
    ],
    "family": "AngularTD",
    "taskRoleArn": "arn:aws:iam::259417942296:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::259417942296:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}